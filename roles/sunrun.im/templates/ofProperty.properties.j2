#SKAPI服务器地址配置--按需要配置
#https访问的ip和端口(默认为8443，如果端口有映射则按需更改)
service.https.hostIp={{ sunrunim.openfire.host }}:8443

#http访问的ip和端口(默认为8080，如果端口有映射则按需更改)
service.hostIp={{ sunrunim.openfire.host }}:8080

#uc服务器的地址(默认为https访问)
service.uc.hostip=
#service.uc.hostip=http://218.107.10.85:10086
#iam服务器
iam.server={{ sunrunim.iam.iam_server }}
iam.access.key={{ sunrunim.iam.iam_access_key }}
iam.access.secret={{ sunrunim.iam.iam_access_secret }}

#openfire服务配置
#openfire服务器的名称，无需修改
