service.xmpp.login.user=admin
service.xmpp.login.password=sunrunvas

#openfire服务配置
#openfire服务p地址
client.xmpp.server={{ sunrunim.openfire.host }}

#openfire服务对应的端口(默认为5222,如果端口有映射则按需更改)
client.xmpp.port=5222

#默认无需修改
